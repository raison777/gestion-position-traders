  <%= csrf_meta_tags %>
  <title>Gestion de transactions</title>
</head>
<body>
  <header>
    <h1>Gestionnaire de transaction</h1>
    <nav>
      <%= link_to :controller => :traders, :action=> :index do -%><b>Traders</b><% end %>&nbsp;|&nbsp;
